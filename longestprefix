
n=int(input())
strings=[]
for _ in range(n):
    strings.append(input())
if n==0:
    print(-1)
else:
    prefix=strings[0]
    for s in strings[1:]:
        i=0
        while i<len(prefix)and i<len(s)and prefix[i]==s[i]:
            i+=1
        prefix=prefix[:i]
        if prefix=="":
            break
    if prefix=="":
        print(-1)
    else:
        print(prefix)

